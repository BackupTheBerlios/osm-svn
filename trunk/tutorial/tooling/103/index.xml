<?xml version="1.0" encoding="ISO-8859-1"?>
<index xmlns="link:xsd:dpml/lang/dpml-module#1.0" 
    xmlns:part="link:xsd:dpml/lang/dpml-part#1.0">

  <imports>
    <import uri="link:module:ant"/>
  </imports>

  <modules>

    <module name="acme">

      <project name="acme-client" basedir="client">
        <types>
          <type id="jar"/>
        </types>
      </project>
    
      <project name="acme-system" basedir="system">
        <types>
          <type id="jar"/>
        </types>
        <dependencies>
          <runtime>
            <include key="acme-client"/>
          </runtime>
          <test>
            <include ref="ant/ant-junit"/>
          </test>
        </dependencies>
        <filters>
          <filter token="COLOR" value="red"/>
          <feature token="NAME" id="name"/>
        </filters>
      </project>
      
    </module>

  </modules>

</index>
